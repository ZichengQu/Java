<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="entity.Order" table="t_order_hibernate">
		<id name="order_id">
			<generator class="assigned"></generator>
		</id>
		<property name="product_id"></property>
		<property name="product_name"></property>
		<property name="product_price"></property>
		<many-to-one name="admin" column="admin_id" class="entity.Admin"></many-to-one>
	</class>
</hibernate-mapping>