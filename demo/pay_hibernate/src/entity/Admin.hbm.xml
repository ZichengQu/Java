<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="entity.Admin" table="t_admin_hibernate">
		<id name="adminId" column="adminId">
			<generator class="native"></generator>
		</id>
		<property name="adminCode" column="adminCode"></property>
		<property name="password"></property>
		<property name="name"></property>
		<property name="telephone"></property>
		<property name="email"></property>
		<property name="enrolldate"></property>
		<set name="orders">
			<key column="admin_id"></key>
			<one-to-many class="entity.Order"/>
		</set>
	</class>
</hibernate-mapping>